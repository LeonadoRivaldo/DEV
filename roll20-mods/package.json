{
	"name": "roll20-mods",
	"version": "1.0.0",
	"description": "",
	"private": true,
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"clean": "node utils/fileManager.js",
		"version": "node utils/versionModule.js",
		"version:store": "node utils/storeVersion.js",
		"classActions:clean:dist": "npm run clean -- modName=class-actions",
		"classActions:version:store": "npm run version:store -- modName=class-actions",
		"classActions:version:patch": "npm run version -- modName=class-actions -- versionType=patch",
		"classActions:version:minor": "npm run version -- modName=class-actions -- versionType=minor",
		"classActions:version:major": "npm run version -- modName=class-actions -- versionType=major",
		"classActions:webpack:build": "webpack --config webpack/classActions.webpack.config.js --mode production",
		"classActions:build": "npm run classActions:webpack:build && npm run classActions:clean:dist",
		"classActions:build:patch": "npm run classActions:version:store && npm run classActions:version:patch && npm run classActions:build",
		"classActions:build:minor": "npm run classActions:version:store && npm run classActions:version:minor && npm run classActions:build",
		"classActions:build:major": "npm run classActions:version:store && npm run classActions:version:major && npm run classActions:build"
	},
	"keywords": [],
	"author": "Leo rivaldo",
	"license": "ISC",
	"dependencies": {
		"generate-json-webpack-plugin": "^2.0.0",
		"prettier": "^3.2.4",
		"typescript": "^5.3.3"
	},
	"devDependencies": {
		"@types/node": "^20.11.6",
		"@typescript-eslint/eslint-plugin": "^6.19.1",
		"@typescript-eslint/parser": "^6.19.1",
		"@webpack-cli/generators": "^3.0.7",
		"eslint": "^8.56.0",
		"terser-webpack-plugin": "^5.3.10",
		"ts-loader": "^9.5.1",
		"ts-node": "^10.9.2",
		"types-roll20": "^2.1.7",
		"webpack": "^5.90.0",
		"webpack-cli": "^5.1.4"
	}
}
